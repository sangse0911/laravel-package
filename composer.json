{
    "name": "sangtm/platform",
    "description": "this package write to custom laravel write by package",
    "prefer-stable": true,
    "license": "proprietary",
    "authors": [
        {
            "name": "sa_tran",
            "email": "sa_tran@vision-net.co.jp"
        }
    ],
    "require": {
        "php": "^8.1",
        "wikimedia/composer-merge-plugin": "^2.1",
        "composer/installers": "~1.0",
        "monolog/monolog": "*"
    },
    "autoload": {
        "psr-4": {
            "Vision\\Plugin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vision\\Plugin\\ExtendServiceProvider"
            ],
            "aliases": {}
        },
        "install-paths": {
            "./monolog/": ["monolog/monolog"]
        }
    },
    "config": {
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        },
        "vendor-dir": "plugins"
    }
}